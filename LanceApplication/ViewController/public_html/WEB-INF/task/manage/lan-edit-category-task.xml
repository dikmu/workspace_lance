<?xml version="1.0" encoding="UTF-8" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="lan-edit-category-task">
    <default-activity>ViewCategory</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <view id="EditCategory">
      <page>/manage/fregment/ViewCategory.jsff</page>
    </view>
    <view id="ViewCategory">
      <page>/manage/fregment/ViewCategory.jsff</page>
    </view>
    <control-flow-rule id="__1">
      <from-activity-id>ViewCategory</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>edit</from-outcome>
        <to-activity-id>EditCategory</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
